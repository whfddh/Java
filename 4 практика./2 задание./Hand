 public  class Hand extends Human{
        private double fingers;
        public Hand (double fingers){
            super();
            this.fingers = fingers;
        }
        public double getFingers(){
            return fingers;
        }
        public void setFingers(){
            this.fingers = fingers;
        }
        public void show(double fingers){
            System.out.println("Человек показывает " + fingers + " пальцев.");
        }

    //public String toString(){
        //return "Circle radius: " + radius + '\n' + "Circle diameter: " + diameter;}

}

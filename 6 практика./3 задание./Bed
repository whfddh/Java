public class Bed extends Furniture{
    private int kolvomest;

    public int getKolvomest() {
        return kolvomest;
    }

    public void setKolvomest(int kolvomest) {
        this.kolvomest = kolvomest;
    }
    public Bed(int size, String color, int kolvomest){
        super(size, color);
        this.kolvomest = kolvomest;
    }
    public void sale(){
        System.out.println("На кровати не действует скидка");
    }
}

public class FurnitureShop {
    public static void main(String[] args) {
        Bed bed = new Bed(12, "white", 2);
        Chair chair = new Chair(5, "blue", false);
        System.out.println("Chair Info: " + chair);
        chair.sale();
        System.out.println("Bed Info: " + bed);
        bed.sale();
    }
}

public abstract class Dog {
    private String name;
    private int age;
    public Dog(String name, int age) {
        this.name = name;
        this.age = age;
    }
    public String getName() {
        return name;
    }
    public int getAge() {
        return age;
    }
    public abstract void givePaw();
    public String toString() {
        return ("Имя собаки: " + name + '\n' + "Возраст собаки: " + age);
    }
}
